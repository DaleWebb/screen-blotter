{
  "name": "ScreenBlotter",
  "description": "Blot out sensitive data when interacting with customer's instances",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "storage"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "ScreenBlotter",
    "default_popup": "popup.html"
  },

  "manifest_version": 2
}
